<Window x:Class="TechnicAppSQL.LoginWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TechnicAppSQL"
        mc:Ignorable="d"
        Title="LoginWindow" Height="395.548" Width="413.699">
    <Grid>
        <Label Content="Personel Login" HorizontalAlignment="Left" Margin="140,96,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.44,0.761" FontSize="18" FontWeight="Bold"/>
        <TextBox x:Name="PersonelTxt" HorizontalAlignment="Left" Height="23" Margin="155,152,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <PasswordBox x:Name="PasswordBox" HorizontalAlignment="Left" Margin="155,194,0,0" VerticalAlignment="Top" Width="120"/>
        <Button Content="Login" HorizontalAlignment="Left" Margin="175,251,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>
        <Label Content="Personel ID:" HorizontalAlignment="Left" Margin="72,150,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.44,0.761"/>
        <Label Content="Password:" HorizontalAlignment="Left" Margin="82,188,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.44,0.761"/>

    </Grid>
</Window>
